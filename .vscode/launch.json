    "version": "0.2.0",
    "configurations": [
        {
            "name": "wNeo_mint",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/wNeo.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": {
                "contract": "NeoToken",
                "operation": "transfer",
                "args": [
                    "@Denis",
                    "@contra",
                    10,
                    "none"
                ]
            },
            "signers": [
                "@Denis"
            ]
        },
        {
            "name": "wNeo_burn",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/wNeo.nef",
            "neo-express": "${workspaceFolder}/default.neo-express",
            "invocation": {
                "operation": "burn",
                "args": [
                    "@Denis",
                    10,
            ]
            },
            "signers": [
                "@Denis"
            ]
        }
    ]
}